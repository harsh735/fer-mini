{
  "name": "image-js",
  "version": "0.21.9",
  "description": "Image processing and manipulation in JavaScript",
  "keywords": [
    "image",
    "processing",
    "manipulation",
    "analysis",
    "histogram",
    "hsb",
    "hsl",
    "rgb",
    "roi",
    "rois",
    "mask"
  ],
  "author": "MichaÃ«l Zasso",
  "contributors": [
    "Luc Patiny"
  ],
  "repository": "image-js/core",
  "bugs": {
    "url": "https://github.com/image-js/core/issues"
  },
  "homepage": "https://github.com/image-js/core",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./src/index.js",
  "types": "./index.d.ts",
  "browser": {
    "./src/worker/worker.js": "./src/browser/worker/worker.js",
    "./src/image/core/environment.js": "./src/browser/environment.js",
    "fs": false
  },
  "files": [
    "dist",
    "lib",
    "src",
    "index.d.ts"
  ],
  "scripts": {
    "build": "npm-run-all clean-dist --parallel compile build-dist",
    "build-dist": "cross-env NODE_ENV=browser rollup -c",
    "build-doc": "documentation build src/index.js --github --output docs --format html --sort-order alpha",
    "clean-dist": "rimraf dist && mkdir dist",
    "clean-lib": "rimraf lib",
    "compile": "npm run clean-lib && cross-env NODE_ENV=npm babel src --out-dir lib --ignore __tests__ --quiet",
    "eslint": "eslint src test/common.js test/nodeScripts --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepublishOnly": "npm run compile",
    "publish-doc": "npm run build-doc && git add docs && git commit -m \"doc: rebuild docs\" && git push origin master",
    "test": "run-s test-only eslint",
    "test-coverage": "jest --coverage --runInBand",
    "test-only": "jest",
    "test-travis": "run-s test-coverage eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-transform-block-scoping": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/register": "^7.8.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "benchmark": "^2.1.4",
    "canvas": "^2.6.1",
    "cross-env": "^6.0.3",
    "documentation": "^12.1.4",
    "eslint": "^6.8.0",
    "eslint-config-cheminfo": "^1.20.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^22.4.1",
    "esm": "^3.2.25",
    "jest": "^25.1.0",
    "jest-matcher-deep-close-to": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "sha.js": "^2.4.11"
  },
  "dependencies": {
    "blob-util": "^2.0.2",
    "canny-edge-detector": "^1.0.0",
    "color-functions": "^3.0.1",
    "fast-bmp": "^1.0.0",
    "fast-jpeg": "^1.0.1",
    "fast-list": "^1.0.3",
    "fast-png": "^5.0.2",
    "has-own": "^1.0.1",
    "image-type": "^4.1.0",
    "is-array-type": "^1.0.0",
    "is-integer": "^1.0.7",
    "jpeg-js": "^0.3.7",
    "js-priority-queue": "^0.1.5",
    "ml-convolution": "^0.2.0",
    "ml-disjoint-set": "^1.0.0",
    "ml-matrix": "^5.2.1",
    "ml-matrix-convolution": "0.4.3",
    "ml-regression": "^4.4.2",
    "monotone-chain-convex-hull": "^1.0.0",
    "new-array": "^1.0.0",
    "num-sort": "^1.0.0",
    "robust-point-in-polygon": "^1.0.3",
    "tiff": "^4.0.0",
    "web-worker-manager": "^0.2.0"
  },
  "engines": {
    "node": ">= 8.0.0"
  }
}
